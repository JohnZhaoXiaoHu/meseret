{
  "name": "meseret",
  "version": "1.7.7",
  "description": "Opinionated, feature-packed professional node.js server framework.",
  "main": "src/main.js",
  "typings": "src/main.d.ts",
  "scripts": {
    "clean": "node_modules/.bin/del src/**/*.{js,d.ts}",
    "build": "yarn clean && yarn tsc && yarn lint",
    "watch": "yarn clean && yarn tsc -w",
    "develop": "nodemon",
    "lint": "prettier --write ./src/**/*.{ts,js} ./*.{json,md}",
    "start": "yarn build && node ."
  },
  "keywords": [
    "meseret",
    "opinionated",
    "professional",
    "node.js",
    "server",
    "framework",
    "backbone",
    "configuration",
    "typescript",
    "http",
    "https",
    "serverapp",
    "koa",
    "koa-router",
    "database",
    "modelfactory",
    "mongoose",
    "mongodb",
    "websocket",
    "socket.io",
    "single page application",
    "spa",
    "404",
    "angular",
    "react"
  ],
  "repository": "git@gitlab.com:kaleabmelkie/meseret",
  "author": "Kaleab Melkie <kaleabmelkie@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">= 8.9",
    "yarn": ">= 1"
  },
  "dependencies": {
    "@types/gridfs-stream": "0.5.30",
    "@types/koa": "2.0.45",
    "@types/koa-compress": "2.0.8",
    "@types/koa-json": "2.0.18",
    "@types/koa-logger": "3.1.0",
    "@types/koa-router": "7.0.28",
    "@types/koa-send": "4.1.1",
    "@types/koa-session": "5.7.3",
    "@types/koa-static": "4.0.0",
    "@types/mongoose": "5.0.13",
    "@types/node": "10.0.8",
    "@types/socket.io": "1.4.33",
    "del-cli": "1.1.0",
    "gridfs-stream": "1.1.1",
    "koa": "2.5.1",
    "koa-body": "2.5.0",
    "koa-compress": "3.0.0",
    "koa-convert": "1.2.0",
    "koa-json": "2.0.2",
    "koa-logger": "3.2.0",
    "koa-router": "7.4.0",
    "koa-send": "4.1.3",
    "koa-session": "5.8.1",
    "koa-static": "4.0.2",
