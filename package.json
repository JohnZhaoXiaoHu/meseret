{
  "name": "meseret",
  "version": "1.7.8",
  "description": "Opinionated, feature-packed professional node.js server framework.",
  "main": "lib",
  "typings": "lib",
  "scripts": {
    "clean": "node_modules/.bin/del lib",
    "build": "yarn clean && tsc",
    "watch": "yarn clean && tsc -w",
    "lint": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "meseret",
    "framework",
    "mongodb",
    "node.js",
    "typescript",
    "mongoose",
    "koa"
  ],
  "repository": "git@gitlab.com:kaleabmelkie/meseret",
  "author": "Kaleab Melkie <kaleabmelkie@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">= 8.9.11",
    "yarn": ">= 1"
  },
  "dependencies": {
    "@types/gridfs-stream": "latest",
    "@types/koa": "latest",
    "@types/koa-compress": "latest",
    "@types/koa-json": "latest",
    "@types/koa-logger": "latest",
    "@types/koa-router": "latest",
    "@types/koa-send": "latest",
    "@types/koa-session": "latest",
    "@types/koa-static": "latest",
    "@types/koa-static-cache": "latest",
    "@types/mongoose": "latest",
    "@types/node": "latest",
    "@types/socket.io": "latest",
    "del-cli": "latest",
    "gridfs-stream": "latest",
    "koa": "latest",
    "koa-body": "latest",
    "koa-compress": "latest",
    "koa-convert": "latest",
    "koa-json": "latest",
    "koa-logger": "latest",
    "koa-router": "latest",
    "koa-send": "latest",
    "koa-session": "latest",
    "koa-static": "latest",
    "koa-static-cache": "latest",
    "mongoose": "latest",
    "nodemon": "latest",
    "prettier": "latest",
    "socket.io": "latest",
    "typescript": "latest"
  }
}
